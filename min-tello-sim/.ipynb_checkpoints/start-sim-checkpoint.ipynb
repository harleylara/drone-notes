    "import http.server\n",
    "import socketserver\n",
    "\n",
    "PORT = 4400\n",
    "\n",
    "handler = http.server.SimpleHTTPRequestHandler\n",
    "\n",
    "with socketserver.TCPServer((\"\", PORT), handler) as httpd:\n",
    "    print(\"Server started at localhost:\" + str(PORT))\n",
    "    httpd.serve_forever()"
   ]
  }
